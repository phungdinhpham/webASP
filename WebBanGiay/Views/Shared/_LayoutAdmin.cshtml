<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/Content/giayy/img/Search-icon.png" />
    <script src="~/tinymce/tinymce.min.js"></script>
    <script src="~/tinymce/jquery.tinymce.min.js"></script>
    <script>tinymce.init({ selector: 'textarea' });</script>
    <meta charset="utf-8">
    <link id="bootstrap-style" href="~/Content/giayy/teamplateAd/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/giayy/teamplateAd/css/bootstrap-responsive.min.css" rel="stylesheet">

    <link id="base-style" href="~/Content/giayy/teamplateAd/css/style.css" rel="stylesheet">
    <link id="base-style-responsive" href="~/Content/giayy/teamplateAd/css/style-responsive.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800&subset=latin,cyrillic-ext,latin-ext' rel='stylesheet' type='text/css'>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <link id="ie-style" href="~/Content/giayy/teamplateAd/css/ie.css" rel="stylesheet">

    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.1/css/all.css" integrity="sha384-50oBUHEmvpQ+1lW4y57PTFmhCaXp0ML5d60M1M7uH2+nqUivzIebhndOJK28anvf" crossorigin="anonymous">
    <link href="~/Content/giayy/teamplateAd/css/style-forms.css" rel="stylesheet" />
    <link href="~/Content/giayy/css/bootstrap.css" rel="stylesheet" />
</head>
<body>
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                @*<a class="btn btn-navbar" data-toggle="collapse" data-target=".top-nav.nav-collapse,.sidebar-nav.nav-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </a>*@
                <a class="brand" href="/Admin/Index"><span>Trang quản lý</span></a>

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav">
                    <ul class="nav pull-right">
                        <li class="dropdown">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="halflings-icon white user"></i>
                                <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li><a href="~/Admin/Index"><i class="halflings-icon user"></i> Trang chủ</a></li>
                                <li><a href="~/Admin/Login"><i class="halflings-icon off"></i> Đăng xuất</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid-full">
        <div class="row-fluid">
            <div id="sidebar-left" class="span1">
                <div class="nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li><a href="~/Admin/Index"><i class="fas fa-home"></i><span> Trang chủ</span></a></li>
                        <li><a href="~/Admin/Giay"><i class="fas fa-mobile-alt"></i><span> Sản phẩm</span></a></li>
                        <li><a href="~/Admin/NhaSanXuat"><i class="far fa-building"></i><span> Nhà sản xuất</span></a></li>
                        <li><a href="~/Admin/DonHang"><i class="far fa-building"></i><span> Đơn đặt hàng</span></a></li>
                        <li><a href="~/Admin/Login"><i class="fas fa-key"></i><span class="hidden-tablet"> Đăng xuất</span></a></li>
                    </ul>
                </div>
            </div>

            <div id="content" class="span10">
                @RenderBody()
            </div>
        </div>
    </div>

    <div class="modal hide fade" id="myModal">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">×</button>
            <h3>Settings</h3>
        </div>
        <div class="modal-body">
            <p>Here settings can be configured...</p>
        </div>
        <div class="modal-footer">
            <a href="#" class="btn" data-dismiss="modal">Close</a>
            <a href="#" class="btn btn-primary">Save changes</a>
        </div>
    </div>

    <div class="clearfix"></div>
    @*<footer>
            <p>
                <span style="text-align:left;float:left">&copy; 2013 <a href="http://jiji262.github.io/Bootstrap_Metro_Dashboard/" alt="Bootstrap_Metro_Dashboard">Bootstrap Metro Dashboard</a></span>

            </p>
        </footer>*@

    <!-- start: JavaScript-->
    <script src="~/Content/giayy/teamplateAd/js/jquery-1.9.1.min.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/jquery-migrate-1.0.0.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery-ui-1.10.0.custom.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.ui.touch-punch.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/modernizr.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/bootstrap.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.cookie.js"></script>

    <script src='~/Content/giayy/teamplateAd/js/fullcalendar.min.js'></script>

    <script src='~/Content/giayy/teamplateAd/js/jquery.dataTables.min.js'></script>
    <script src="~/Content/giayy/teamplateAd/js/excanvas.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/jquery.flot.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/jquery.flot.pie.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/jquery.flot.stack.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/jquery.flot.resize.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.chosen.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.uniform.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.cleditor.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.noty.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.elfinder.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.raty.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.iphone.toggle.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.uploadify-3.1.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.gritter.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.imagesloaded.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.masonry.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.knob.modified.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/jquery.sparkline.min.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/js/counter.js"></script>

    <script src="~/Content/giayy/teamplateAd/js/js/retina.js"></script>
    <script src="~/Content/giayy/teamplateAd/js/custom.js"></script>
    <!-- end: JavaScript-->

</body>
</html>

